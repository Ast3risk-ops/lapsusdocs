{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Lapsus Development",
  "logo": {
    "dark": "/assets/img/brand/logo.png",
    "light": "/assets/img/brand/logo.png"
  },
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#000000",
    "light": "#ffffff",
    "dark": "#ffffff",
    "anchors": {
      "from": "#0e0e10",
      "to": "#0e0e10"
    }
  },
  "feedback": {
    "suggestEdit": true,
    "raiseIssue": true
  },
  "codeBlock": {
    "mode": "auto"
  },
  "topbar": {
    "style": "gradient"
  },
  "layout": "topnav",
  "background": {
    "style": "windows"
  },
  "sidebar": {
    "items": "container"
  },
  "primaryTab": {
    "name": "Welcome"
  },
  "topbarCtaButton": {
    "type": "github",
    "url": "https://github.com/ManucrackYT/LapsusClient"
  },
  "topAnchor": {
    "name": "Home",
    "icon": "house-chimney",
    "iconType": "duotone"
  },
  "anchors": [
    {
      "name": "Lapsus Client",
      "icon": "server",
      "iconType": "duotone",
      "url": "client"
    },
    {
      "name": "Lapsus Launcher",
      "icon": "gamepad-modern",
      "iconType": "duotone",
      "url": "launcher"
    }
  ],
  "navigation": [
    {
      "group": "",
      "pages": [
        "welcome"
      ]
    },
    {
      "group": "",
      "pages": [
        "client/introduction",
        "client/quickstart",
        "client/installation",
        {
          "group": "Reference",
          "icon": "gear",
          "iconType": "duotone",
          "pages": [
            "client/reference/config",
            "client/reference/os-support"
          ]
        }
      ]
    },
    {
      "group": "",
      "pages": [
        "launcher/introduction"
      ]
    }
  ],
  "footerSocials": {
    "github": "https://github.com/ManuCrackYT/LapsusClient",
    "discord": "https://discord.gg/dFq6V4MSKN"
  }
}
